
import java.util.*;

class Program {
  public static boolean isValidSubsequence(List<Integer> array, List<Integer> sequence) 
	{
    // Write your code here.
    	int arr=0;
	int sq=0;
	while (sq<sequence.length){
	    if(sq<=arr){
	        for (int m=0;m<array.length;m++){
	            if(sq>=len(sequence)){
	                break;
								}
	            if(array[m]==sequence[sq]){
	                sq=sq+1;
	                arr=m;
								}
					}
			}	                
	            
	    break;
		}
	if(sq==len(sequence)){
		return (True);
		}
	else{
		return (False);
  }
}
}
